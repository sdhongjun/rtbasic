{
  "name": "rtbasic",
  "displayName": "RtBasic",
  "description": "正运动RTBasic语言",
  "version": "0.0.1",
  "engines": {
    "vscode": "*"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rtbasic",
      "aliases": ["RtBasic", "rtbasic"],
      "extensions": [".bas"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rtbasic",
      "scopeName": "source.bas",
      "path": "./syntaxes/rtbasic.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "rtbasic",
        "path": "./snippets/rtbasic.code-snippets"
      }
    ]
  }
}
